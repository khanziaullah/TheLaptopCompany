@model WebUI.Models.LaptopsViewModel

@{
    ViewBag.Title = "Laptop & Ultrabooks";
}
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
            </hgroup>
            <p>
                Check a wide range of laptops and ultrabook at discounted prices.
            </p>
        </div>
    </section>
}

@Html.Partial("_laptops", this.Model.Laptops)